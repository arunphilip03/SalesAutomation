<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.2.66.68" id="ContactDetailPageDef"
                Package="com.sfa.view.pageDefs">
  <parameters/>
  <executables>
