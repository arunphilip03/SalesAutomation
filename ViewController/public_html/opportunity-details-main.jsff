                                            </af:button>
                                            <af:button text="Attachments" id="b9" action="attachment"/>
                                        </af:toolbar>
                                    </f:facet>
                                    <f:facet name="legend"/>
                                    <f:facet name="info"/>
                                </af:panelHeader>
                            </f:facet>
                            <f:facet name="second">
                                <af:panelFormLayout id="pfl1" rows="5" fieldWidth="300px" labelWidth="75px">
                                    <af:inputText value="#{bindings.OpportunityName.inputValue}"
