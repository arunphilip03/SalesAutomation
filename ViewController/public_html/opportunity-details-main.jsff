                                            <af:link id="l1" icon="#{resource['images:new_ena.png']}"
                                                     actionListener="#{bindings.Create.execute}"
                                                     action="revenueCreate"/>
                                            <af:link id="l2" icon="#{resource['images:update_ena.png']}"
                                                     action="revenueEdit"
                                                     disabledIcon="#{resource['images:update_dis.png']}"
                                                     disabled="#{bindings.OpportunityRevenueItemsView2Iterator.estimatedRowCount eq 0}"/>
                                            <af:popup childCreation="deferred" autoCancel="disabled" id="deletep">
                                                <af:dialog id="d1"
                                                           title="Are you sure you want to delete the Selected Revenue Item?"
                                                           dialogListener="#{ConfirmBean.dialogListener}">
                                                    <f:facet name="buttonBar"/>
                                                </af:dialog>
                                            </af:popup>
                                            <af:link id="l3" icon="#{resource['images:delete_ena.png']}"
                                                     disabledIcon="#{resource['images:delete_dis.png']}"
                                                     disabled="#{bindings.OpportunityRevenueItemsView2Iterator.estimatedRowCount eq 0}">
                                                <af:showPopupBehavior popupId="deletep" triggerType="click"
                                                                      align="overlap"/>
                                            </af:link>
                                        </af:toolbar>
                                    </f:facet>
