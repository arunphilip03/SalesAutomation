                                <af:resetActionListener/>
                            </af:button>
                        </af:toolbar>
                    </f:facet>
                    <f:facet name="legend"/>
                    <f:facet name="info"/>
                </af:panelHeader>
                <!-- Content -->
                <af:panelSplitter id="ps1" orientation="vertical" splitterPosition="203">
                    <f:facet name="first">
