<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="opportunity-task-main-tf">
    <default-activity>opportunity-task-main</default-activity>
    <view id="opportunity-task-main">
      <page>/opportunity-task-main.jsff</page>
    </view>
    <view id="opportunity-task-create">
      <page>/opportunity-task-create.jsff</page>
    </view>
    <view id="opportunity-task-edit">
      <page>/opportunity-task-edit.jsff</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>opportunity-task-main</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>create</from-outcome>
        <to-activity-id>opportunity-task-create</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>edit</from-outcome>
        <to-activity-id>opportunity-task-edit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>exit</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>opportunity-task-create</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>back</from-outcome>
        <to-activity-id>opportunity-task-main</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>opportunity-task-edit</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>back</from-outcome>
        <to-activity-id>opportunity-task-main</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
