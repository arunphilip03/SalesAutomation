<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="opportunity-revenue-tf">
    <default-activity>opportunity-revenue-main</default-activity>
    <view id="opportunity-revenue-main">
      <page>/opportunity-revenue-main.jsff</page>
    </view>
    <view id="opportunity-revenue-create">
      <page>/opportunity-revenue-create.jsff</page>
    </view>
    <view id="opportunity-revenue-edit">
      <page>/opportunity-revenue-edit.jsff</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>opportunity-revenue-main</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>revenueCreate</from-outcome>
        <to-activity-id>opportunity-revenue-create</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>revenueEdit</from-outcome>
        <to-activity-id>opportunity-revenue-edit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__8">
        <from-outcome>back</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__3">
      <from-activity-id>opportunity-revenue-create</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome>revCBack</from-outcome>
        <to-activity-id>opportunity-revenue-main</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>opportunity-revenue-edit</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>revEBack</from-outcome>
        <to-activity-id>opportunity-revenue-main</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
