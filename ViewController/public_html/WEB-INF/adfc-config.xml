<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="SalesDashboard">
    <page>/SalesHome.jsf</page>
  </view>
  <view id="AdminDashboard">
    <page>/AdminDashboard.jsf</page>
  </view>
  <view id="Opportunities">
    <page>/Opportunities.jsf</page>
  </view>
  <control-flow-rule id="__1">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__6">
      <from-outcome>goAdmin</from-outcome>
      <to-activity-id>AdminDashboard</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <control-flow-rule id="__4">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__7">
      <from-outcome>goOpportunity</from-outcome>
      <to-activity-id>Opportunities</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__3">
    <managed-bean-name>launcher</managed-bean-name>
    <managed-bean-class>com.sfa.view.backing.Launcher</managed-bean-class>
    <managed-bean-scope>backingBean</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__1">
    <managed-bean-name>InviteeBean</managed-bean-name>
    <managed-bean-class>com.sfa.view.backing.InviteeBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean id="__2">
    <managed-bean-name>TaskTeamBean</managed-bean-name>
    <managed-bean-class>com.sfa.view.backing.TaskTeamBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</adfc-config>
