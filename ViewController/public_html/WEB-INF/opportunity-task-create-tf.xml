<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="opportunity-task-create-tf">
    <default-activity>opportunity-task-create</default-activity>
    <transaction>
      <requires-transaction/>
    </transaction>
    <view id="opportunity-task-create">
      <page>/opportunity-task-create.jsff</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
        <rollback/>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>opportunity-task-create</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>back</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
