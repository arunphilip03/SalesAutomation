<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="edit-contact-taskflow">
    <default-activity>ContactEdit</default-activity>
    <transaction>
      <requires-transaction/>
    </transaction>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="ContactEdit">
      <page>/ContactEdit.jsf</page>
    </view>
    <task-flow-return id="Save">
      <outcome>
        <name>Save</name>
        <commit/>
      </outcome>
    </task-flow-return>
    <task-flow-return id="Cancel">
      <outcome>
        <name>Cancel</name>
        <rollback/>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>ContactEdit</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>save</from-outcome>
        <to-activity-id>Save</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>Cancel</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
