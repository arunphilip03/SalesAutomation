<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="Sales-repHome-tf">
    <default-activity>ExecuteWithParams</default-activity>
    <view id="SalesRep-home">
      <page>/SalesRep-home.jsff</page>
    </view>
    <view id="SalesRep-other">
      <page>/SalesRep-other.jsff</page>
    </view>
    <method-call id="ExecuteWithParams1">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="ExecuteWithParams">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__11">
      <from-activity-id>ExecuteWithParams1</from-activity-id>
      <control-flow-case id="__12">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>SalesRep-other</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__15">
      <from-activity-id>SalesRep-home</from-activity-id>
      <control-flow-case id="__16">
        <from-outcome>other</from-outcome>
        <to-activity-id>ExecuteWithParams1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__17">
      <from-activity-id>ExecuteWithParams</from-activity-id>
      <control-flow-case id="__18">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>SalesRep-home</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__1">
      <from-activity-id>SalesRep-other</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>back</from-outcome>
        <to-activity-id>SalesRep-home</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
