<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="opportunities-appointment-tf">
    <default-activity>opportunities-appoinment-main</default-activity>
    <view id="opportunities-appoinment-main">
      <page>/opportunities-appoinment-main.jsff</page>
    </view>
    <view id="opportunities-appoinment-create">
      <page>/opportunities-appoinment-create.jsff</page>
    </view>
    <view id="opportunities-appoinment-edit">
      <page>/opportunities-appoinment-edit.jsff</page>
    </view>
    <task-flow-return id="taskFlowReturn1">
      <outcome>
        <name>taskFlowReturn1</name>
      </outcome>
    </task-flow-return>
    <task-flow-call id="opportunities-appoinment-create-tf">
      <task-flow-reference>
        <document>/WEB-INF/opportunities-appoinment-create-tf.xml</document>
        <id>opportunities-appoinment-create-tf</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__1">
      <from-activity-id>opportunities-appoinment-main</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>edit</from-outcome>
        <to-activity-id>opportunities-appoinment-edit</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__3">
        <from-outcome>create</from-outcome>
        <to-activity-id>opportunities-appoinment-create</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__4">
        <from-outcome>exit</from-outcome>
        <to-activity-id>taskFlowReturn1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>opportunities-appoinment-edit</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>back</from-outcome>
        <to-activity-id>opportunities-appoinment-main</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__7">
      <from-activity-id>opportunities-appoinment-create</from-activity-id>
      <control-flow-case id="__8">
        <from-outcome>back</from-outcome>
        <to-activity-id>opportunities-appoinment-main</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
