                                               width="128">
                                        <af:outputText shortDesc="#{bindings.CustomerContactsView2.hints.ContactName.tooltip}"
                                                       id="ot33" value="#{row.ContactName}"/>
                                    </af:column>
                                    <af:column headerText="#{bindings.CustomerContactsView2.hints.Title.label}"
                                               sortable="true" id="c3"
