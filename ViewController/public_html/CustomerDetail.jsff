                                          styleClass="AFStretchWidth" columnStretching="last">
                                    <af:column sortProperty="#{bindings.TasksView3.hints.TaskName.name}" sortable="true"
                                               headerText="#{bindings.TasksView3.hints.TaskName.label}" id="c14"
                                               width="246">
