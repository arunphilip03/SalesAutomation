                        </af:showDetailItem>
                        <af:showDetailItem text="Tasks" id="sdi3" styleClass="AFStretchWidth">
                            <af:panelCollection id="pc3" styleClass="AFStretchWidth">
                                <f:facet name="menus"/>
                                <f:facet name="toolbar">
