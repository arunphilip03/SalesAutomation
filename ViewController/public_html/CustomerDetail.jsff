                                        </af:outputText>
                                    </af:column>
                                    <af:column headerText="#{bindings.AppointmentsView2.hints.EndDatetime.label}"
                                               id="c11"
                                               sortProperty="#{bindings.AppointmentsView2.hints.EndDatetime.name}"
                                               sortable="true" width="164">
