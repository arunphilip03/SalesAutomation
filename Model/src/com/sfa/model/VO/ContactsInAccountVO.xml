<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="ContactsInAccountVO"
  Version="12.1.2.66.68"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.sfa.model.VO.ContactsInAccountVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="ACCOUNTID"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT CustomerContacts.CONTACT_NAME,   
       CustomerContacts.CONTACT_ID,   
       CustomerContacts.OBJECT_VERSION_NUMBER,   
       CustomerContacts.EMAIL  
FROM  CUSTOMER_CONTACTS CustomerContacts  
WHERE CustomerContacts.CUST_ACCOUNT_ID =:ACCOUNTID]]></SQLQuery>
  <EntityUsage
    Name="CustomerContacts"
    Entity="com.sfa.model.entity.CustomerContacts"
    ReadOnly="true"
    DeleteParticipant="false"/>
  <ViewAttribute
    Name="ContactName"
    IsUpdateable="false"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ContactName"
    EntityUsage="CustomerContacts"
    AliasName="CONTACT_NAME"/>
  <ViewAttribute
    Name="ContactId"
    IsUpdateable="while_insert"
    PrecisionRule="true"
    EntityAttrName="ContactId"
    EntityUsage="CustomerContacts"
    AliasName="CONTACT_ID"/>
  <ViewAttribute
    Name="ObjectVersionNumber"
    PrecisionRule="true"
    EntityAttrName="ObjectVersionNumber"
    EntityUsage="CustomerContacts"
    AliasName="OBJECT_VERSION_NUMBER"/>
  <ViewAttribute
    Name="Email"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Email"
    EntityUsage="CustomerContacts"
    AliasName="EMAIL"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.sfa.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
